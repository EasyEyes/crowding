{
  "name": "threshold",
  "version": "0.0.1",
  "private": "true",
  "description": "Threshold Participant.",
  "main": "threshold.js",
  "scripts": {
    "setup:psychojs": "cd psychojs && git checkout threshold-prod && npm ci && cd ..",
    "format": "prettier --write \"**/*.{js,css,html,md,ts}\"",
    "phrases": "node server/fetch-languages-sheets.js && git add components/i18n.js",
    "glossary": "node server/prepare-glossary.js && prettier --write parameters/glossary.ts && git add parameters/glossary.ts",
    "prepare": "husky install && npm run setup:psychojs",
    "start": "webpack --env=development",
    "build": "webpack --env=production",
    "examples": "sh server/examples.sh",
    "netlify": "git submodule update --init --recursive && npm run setup:psychojs && npm run build && npm run examples",
    "netlify:website": "npm run setup:psychojs && npm ci && npm run build && rm -r node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyEyes/threshold.git"
  },
  "keywords": [
    "psychology"
  ],
  "author": "EasyEyes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EasyEyes/threshold/issues"
  },
  "homepage": "https://github.com/EasyEyes/threshold#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@types/papaparse": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "dataframe-js": "^1.4.4",
    "eslint": "^8.4.1",
    "googleapis": "^92.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "xlsx": "^0.17.4"
  },
  "lint-staged": {
    "*.{js,css,html,md,ts}": "prettier --write"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "papaparse": "^5.3.1",
    "webfontloader": "^1.6.28"
  }
}
